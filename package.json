{
  "name": "daytrippr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "db:start": "supabase start -x gotrue,realtime,storage-api,imgproxy,edge-runtime,vector,logflare,mailpit",
    "db:stop": "supabase stop",
    "db:stop:all": "supabase stop --all",
    "db:reset": "supabase db reset",
    "db:diff": "supabase db diff -f change",
    "db:shell": "docker exec -it supabase_db_daytrippr psql -U postgres -d postgres",
    "db:seed:manual": "docker cp supabase/seed.sql supabase_db_daytrippr:/seed.sql && docker exec -it supabase_db_daytrippr psql -U postgres -d postgres -v ON_ERROR_STOP=1 -f /seed.sql",
    "cloud:login": "supabase login",
    "cloud:link": "supabase link --project-ref <your-project-ref>",
    "cloud:push": "supabase db push"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.18.0",
    "@hookform/resolvers": "^5.2.1",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.54.0",
    "@tanstack/react-query": "^5.85.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.539.0",
    "next": "^15.4.6",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^15.4.6",
    "husky": "^8.0.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "supabase": "^2.34.3",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
}
